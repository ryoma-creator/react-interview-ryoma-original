function removeDuplicates(arr) {
  // Set ã‚’ä½¿ã£ã¦é‡è¤‡ã‚’æŽ’é™¤
  const uniqueSet = new Set(arr);
  
  // Set ã‚’é…åˆ—ã«å¤‰æ›
  return Array.from(uniqueSet);
  
  // åˆ¥ã®æ–¹æ³•: return [...uniqueSet];
}

// ãƒ†ã‚¹ãƒˆ
console.log(removeDuplicates([1, 2, 2, 3, 4, 4, 5])); // [1, 2, 3, 4, 5]
