function countVowels(str) {
  const vowels = "aeiou".split('');
  const chars = str.toLowerCase().split('');
  const vowelsOnly = chars.filter(char => vowels.includes(char));
  
  return {
    count: vowelsOnly.length,
    foundVowels: vowelsOnly
  };
}

return { count: vowelsOnly.length, foundVowels: vowelsOnly }; ã«ã¤ã„ã¦
ãã®ã¨ãŠã‚Šã§ã™ã€‚ã“ã®æ›¸ãæ–¹ã§ã‚ªãƒ–ã‚¸ã‚§ã‚¯ãƒˆã‚’è¿”ã™ã“ã¨ã§ã€è¤‡æ•°ã®å€¤ã‚’åŒæ™‚ã«è¿”ã›ã¾ã™ã€‚
ä¾‹ãˆã°ä»¥ä¸‹ã®ã‚ˆã†ã«ä½¿ã„ã¾ã™ï¼š
