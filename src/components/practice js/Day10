function doubleAfterDelay(number, callback) {
  // ã“ã“ã«ã‚³ãƒ¼ãƒ‰ã‚’æ›¸ã„ã¦ãã ã•ã„
  
}

// ãƒ†ã‚¹ãƒˆ
