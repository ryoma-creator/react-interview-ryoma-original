function processNumber(number) {
  // ã“ã“ã«ã‚³ãƒ¼ãƒ‰ã‚’æ›¸ã„ã¦ãã ã•ã„
  
}

// ãƒ†ã‚¹ãƒˆ
processNumber(5)
  .then(result => {
    console.log("å•é¡Œ3: æœ€çµ‚çµæœ = " + result); // (5*2 + 10)/2 = 10
  });

// å•é¡Œ4: Promise.all
// 3ã¤ã®éåŒæœŸæ“ä½œã‚’åŒæ™‚ã«å®Ÿè¡Œã—ã€å…¨ã¦ã®çµæœã‚’é…åˆ—ã§å–å¾—ã™ã‚‹é–¢æ•°ã‚’ä½œæˆã—ã¦ãã ã•ã„
function runParallel(value1, value2, value3) {
  // ã“ã“ã«ã‚³ãƒ¼ãƒ‰ã‚’æ›¸ã„ã¦ãã ã•ã„
  
}

// å„å€¤ã‚’å€ã«ã™ã‚‹éåŒæœŸé–¢æ•°
function asyncDouble(value) {
  return new Promise((resolve) => {
    setTimeout(() => {
      resolve(value * 2);
    }, Math.random() * 1000); // 0~1ç§’ã®ãƒ©ãƒ³ãƒ€ãƒ ãªé…å»¶
  });
}

// ãƒ†ã‚¹ãƒˆ
runParallel(5, 10, 15)
  .then(results => {
    console.log("å•é¡Œ4: å…¨ã¦ã®çµæœ:", results); // [10, 20, 30]
  });

// å•é¡Œ5: Promise.race
// 2ã¤ã®éåŒæœŸæ“ä½œã®ã†ã¡ã€æœ€åˆã«å®Œäº†ã—ãŸçµæœã‚’è¿”ã™é–¢æ•°ã‚’ä½œæˆã—ã¦ãã ã•ã„
function runRace(value1, value2) {
  // ã“ã“ã«ã‚³ãƒ¼ãƒ‰ã‚’æ›¸ã„ã¦ãã ã•ã„
  
}

// å€¤ã‚’æŒ‡å®šã—ãŸç§’æ•°å¾Œã«è¿”ã™éåŒæœŸé–¢æ•°
function delayedValue(value, seconds) {
  return new Promise((resolve) => {
    setTimeout(() => {
      resolve(value);
    }, seconds * 1000);
  });
}

// ãƒ†ã‚¹ãƒˆ
runRace(
  delayedValue("é€Ÿã„å‡¦ç†", 1),
  delayedValue("é…ã„å‡¦ç†", 2)
).then(result => {
  console.log("å•é¡Œ5: æœ€åˆã®çµæœ:", result); // "é€Ÿã„å‡¦ç†"
});

// å•é¡Œ6: async/await ã®åŸºæœ¬
// å•é¡Œ2ã¨åŒã˜æ©Ÿèƒ½ã‚’async/awaitã‚’ä½¿ã£ã¦å®Ÿè£…ã—ã¦ãã ã•ã„
// æ•°å€¤ã‚’å—ã‘å–ã‚Šã€1ç§’å¾Œã«ãã®æ•°å€¤ã‚’2å€ã«ã—ã¦è¿”ã™Promiseã‚’ä½œæˆã—ã¦ãã ã•ã„
async function doubleAfterDelayAsync(number) {
  // ã“ã“ã«ã‚³ãƒ¼ãƒ‰ã‚’æ›¸ã„ã¦ãã ã•ã„
  
}

// ãƒ†ã‚¹ãƒˆ
