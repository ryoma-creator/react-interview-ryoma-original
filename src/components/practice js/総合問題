// å•é¡Œ8: åŸºæœ¬çš„ãªasync/awaitã®ä½¿ç”¨
async function delayedResponseAsync() {
  const delay = ms => new Promise(resolve => setTimeout(resolve, ms));
  await delay(1000);
  return "å®Œäº†";
}

